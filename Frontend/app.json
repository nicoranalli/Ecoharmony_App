{
  "expo": {
    "name": "react-native-app",
    "slug": "react-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ecoharmony",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "fonts": [
      {
        "asset": "./assets/fonts/Montserrat-Regular.ttf",
        "name": "MyFontRegular"
      }],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuempresa.ecoharmony",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["ecoharmony"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.yourapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "mi-app",
              "host": "payment"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]}]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}